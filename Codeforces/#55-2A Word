int main() {
    std::ios_base::sync_with_stdio(false);
    cin.tie(0);
    int k;
    string a;
    cin>>a;
    string b;
    b=a;
    sort(b.begin(),b.end());
    //cout<<b<<b[b.size()/2]<<endl;
    if(b[b.size()/2]<='Z') {
       // /cout<<"cap"<<endl;
        for(int i=0;i<a.size();i++){
            if('a'<=a[i]) a[i]-=32;
        }
        
    }
    else{
        //cout<<"baap"<<endl;
        for(int i=0;i<a.size();i++){
            if(a[i]<='Z') a[i]+=32;
        }
        
    }
    cout<<a<<endl;
    return 0;
    
    
}

